    pvs_count = 1;
    if (!$.cookie('m_session')) {
       session_count = 1;
    } else {
       session_count = $.cookie('m_session');
       session_count ++;
    }
    $.cookie( "m_session", session_count, { expires: 30, path: "/" });
  } else {
    pvs_count = $.cookie('m_session_pvs');
    pvs_count ++;
  }
  date.setTime( date.getTime() + ( 30 * 60 * 1000 ));
  $.cookie( "m_session_pvs", pvs_count, { expires: date, path: "/" });
});
